# Print the matrix, row by row
for row in matrix:
    print(' '.join(f'{val:5}' for val in row))

trimmed_matrix = [x[1:] for x in matrix]

import matplotlib.pyplot as plt
plt.imshow(trimmed_matrix)
plt.show()